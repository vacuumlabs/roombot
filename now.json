{
    "version": 2,
    "name": "roombot",
    "alias": "room.vacuumlabs.com",
    "builds": [
        { "src": "index.js", "use": "@now/node-server" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "index.js" }
      ],
    "env": {
        "SLACK_SIGNING_SECRET": "@roombot-slack-signing-secret",
        "GOOGLE_PRIVATE_KEY_JSON": "@roombot-google-private-key-json",
        "PORT": "3000"
    }
}