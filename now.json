{
    "version": 2,
    "name": "roombot",
    "alias": "roombot-vacuumlabs.now.sh",
    "builds": [
        { "src": "app.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "app.js" }
      ],
    "env": {
        "GOOGLE_OAUTH_REFRESH_TOKEN": "@google-refresh-token",
        "GOOGLE_CLIENT_ID": "@google-client-id",
        "GOOGLE_CLIEN_SECRET": "@google-client-secret",
        "GOOGLE_OAUTH_REDIRECT_URL": "https://roombot-vacuumlabs.herokuapp.com/recievecode",
        "SLACK_SIGNING_SECRET": "@slack-signing-secret",
        "PORT": "3000"
    }
}